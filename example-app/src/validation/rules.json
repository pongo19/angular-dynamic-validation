{
  "exampleModel": {
    "fields": {
      "name": {
        "rules": {
          "required": {
            "required": true,
            "message": "This field is required."
          },
          "length": {
            "min": 0,
            "max": 50,
            "message": "Max length is 50"
          }
        }
      },
      "surname": {
        "rules": {
          "custom-validator": {
            "message": "This is custom validator message",
            "ignoreCase": false
          }
        }
      }
    }
  }
}
